{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "keyofStringsOnly": true,
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
            ]
        },
        "lib": [
            "es2017",
            "dom",
            "esnext.asynciterable"
        ]
    },
    "include": [
        "*"
    ]
}
